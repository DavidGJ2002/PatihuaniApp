            tools:srcCompat="@tools:sample/avatars"
            android:contentDescription="Imagen de Producto Saludable 4" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="242dp"
            android:layout_marginTop="587dp"
            android:text="Producto Saludable 3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button7"
            android:layout_width="129dp"
            android:layout_height="60dp"
            android:layout_marginStart="252dp"
            android:layout_marginTop="606dp"
            android:text="Informacion Nutricional"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="53dp"
            android:layout_marginTop="81dp"
            android:text="tus Dietas"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/customButton"
            android:layout_width="95dp"
            android:layout_height="64dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="372dp"
            android:background="@android:color/white"

            android:paddingStart="8dp"
            android:paddingTop="16dp"
            android:paddingEnd="8dp"
            android:paddingBottom="16dp"
            android:text="Frutas"
            android:textColor="@android:color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btnVerduras"
            android:layout_width="95dp"
            android:layout_height="64dp"
            android:layout_marginStart="280dp"
            android:layout_marginTop="376dp"
            android:background="@android:color/white"

            android:paddingStart="8dp"
            android:paddingTop="16dp"
            android:paddingEnd="8dp"
            android:paddingBottom="16dp"
            android:text="Verduras"
            android:textColor="@android:color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>
