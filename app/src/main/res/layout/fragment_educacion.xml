<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   tools:context=".EducacionNutricional">

 <androidx.constraintlayout.widget.ConstraintLayout
     android:layout_width="match_parent"
     android:layout_height="match_parent">

     <LinearLayout
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:orientation="horizontal"
         android:gravity="start"
         android:padding="10dp"
         tools:ignore="MissingConstraints"></LinearLayout>
     <!-- Botón de regreso -->
     <!-- Título -->
     <Button
         android:id="@+id/button_back"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_gravity="start"
         android:layout_margin="16dp"
         android:layout_marginStart="16dp"
         android:text="Regresar"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toTopOf="parent" />

     <LinearLayout
         android:layout_width="158dp"
         android:layout_height="201dp"
         android:layout_marginTop="148dp"
         android:orientation="vertical"
         app:layout_constraintTop_toTopOf="parent"
         tools:layout_editor_absoluteX="33dp"
         tools:ignore="MissingConstraints">

         <ImageView
             android:id="@+id/imageView5"
             android:layout_width="match_parent"
             android:layout_height="122dp"
             app:srcCompat="@drawable/etiquetado" />

         <TextView
             android:id="@+id/textView2"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_gravity="start"
             android:text="Empaquetado"
             app:layout_constraintBottom_toBottomOf="parent"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="parent" />

         <Button
             android:id="@+id/button8"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:background="@android:color/white"
             android:text="Informacion"
             android:layout_gravity="start"
             android:textColor="@android:color/black" />

     </LinearLayout>

     <LinearLayout
         android:layout_width="152dp"
         android:layout_height="206dp"
         android:orientation="vertical"
         tools:layout_editor_absoluteX="233dp"
         tools:layout_editor_absoluteY="148dp"
         tools:ignore="MissingConstraints">

         <ImageView
             android:id="@+id/imageView6"
             android:layout_width="match_parent"
             android:layout_height="130dp"
             app:srcCompat="@drawable/hipertencion" />

         <TextView
             android:id="@+id/textView9"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_gravity="start"
             android:text="Hipertencion"
             app:layout_constraintBottom_toBottomOf="parent"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="parent" />

         <Button
             android:id="@+id/button9"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:background="@android:color/white"
             android:text="Informacion"
             android:layout_gravity="start"
             android:textColor="@android:color/black" />

     </LinearLayout>

     <LinearLayout
         android:layout_width="152dp"
         android:layout_height="206dp"
         android:orientation="vertical"
         tools:layout_editor_absoluteX="39dp"
         tools:layout_editor_absoluteY="395dp"
         tools:ignore="MissingConstraints">

         <ImageView
             android:id="@+id/imageView11"
             android:layout_width="match_parent"
             android:layout_height="130dp"
             app:srcCompat="@drawable/lacteos" />

         <TextView
             android:id="@+id/textView10"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_gravity="start"
             android:text="Uso de Lacteos"
             app:layout_constraintBottom_toBottomOf="parent"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="parent" />

         <Button
             android:id="@+id/button10"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_gravity="start"
             android:background="@android:color/white"
             android:text="Informacion"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="parent"
             android:textColor="@android:color/black" />
     </LinearLayout>

     <LinearLayout
         android:layout_width="152dp"
         android:layout_height="206dp"
         android:orientation="vertical"
         tools:layout_editor_absoluteX="233dp"
         tools:layout_editor_absoluteY="395dp"
         tools:ignore="MissingConstraints">

         <ImageView
             android:id="@+id/imageView12"
             android:layout_width="match_parent"
             android:layout_height="130dp"
             app:srcCompat="@drawable/pan" />

         <TextView
             android:id="@+id/textView11"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:layout_gravity="start"
             android:text="Noticia"

             app:layout_constraintBottom_toBottomOf="parent"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="parent" />

         <Button
             android:id="@+id/button11"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:background="@android:color/white"
             android:text="Informacion"
             android:gravity="start"
             android:textColor="@android:color/black" />
     </LinearLayout>

     <TextView
         android:id="@+id/textView12"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:text="Noticias "
         tools:ignore="MissingConstraints"
         tools:layout_editor_absoluteX="33dp"
         tools:layout_editor_absoluteY="95dp"
         android:layout_gravity="start"/>

     <Button
         android:id="@+id/button12"
         android:layout_width="239dp"
         android:layout_height="39dp"
         android:text="Educacion Nutricional"
         android:textColor="@android:color/black"
         tools:ignore="MissingConstraints"
         tools:layout_editor_absoluteX="125dp"
         tools:layout_editor_absoluteY="1dp" />

     <Button
         android:id="@+id/button2"
         android:layout_width="120dp"
         android:layout_height="48dp"
         android:layout_marginStart="264dp"
         android:layout_marginTop="84dp"

         android:text="Ver Mas"
         android:textColor="@android:color/black"
         app:layout_constraintStart_toStartOf="parent"
         app:layout_constraintTop_toTopOf="parent" />
 </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>